    "    epochs=epochs,\n",
    "    validation_split=0.1, # Optional: use part of training data for validation during training\n",
    "    callbacks=[early_stopping]\n",
    "    # Or use validation_data=(xtest, ytest_one_hot) - be careful not to \"tune\" on test set\n",
    ")\n",
    "\n",
