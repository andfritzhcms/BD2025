    "@tf.function\n",
    "def get_embedding(waveform):\n",
    "    waveform = waveform / tf.int16.max\n",
    "    _, embedding, _ = model_yamnet(waveform)\n",
    "    return embedding\n",
    "\n",
    "audio_embeddings = []\n",
